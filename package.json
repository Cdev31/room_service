{
  "name": "backend_nodejs",
  "version": "1.0.0",
  "description": "'backend para el proyecto Room'",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js ",
    "build": "tsc",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
